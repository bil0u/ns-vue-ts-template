{
  "name": "ns-vue-ts-template",
  "version": "1.0.0",
  "description": "NativeScript Application",
  "author": "Ugo Pop√©e <devteam@p0p.ee>",
  "license": "None",
  "nativescript": {
    "id": "org.nativescript.nsvuetstemplate",
    "tns-android": {
      "version": "6.5.3"
    },
    "tns-ios": {
      "version": "6.5.2"
    }
  },
  "scripts": {
    "clean-run:ios": "tns run ios --bundle --clean",
    "clean-run:android": "tns run android --bundle --clean",
    "start:ios": "tns run ios --bundle",
    "start:android": "tns run android --bundle",
    "dev:ios": "vue-devtools& tns run ios --bundle",
    "dev:android": "vue-devtools& tns run android --bundle",
    "watch:ios": "tns run ios --bundle --hmr",
    "watch:android": "tns run android --bundle --hmr",
    "build:ios": "tns build ios --bundle --production",
    "build:android": "tns build android --bundle --production"
  },
  "dependencies": {
    "@nativescript/theme": "~2.3.3",
    "@vue/devtools": "^5.3.3",
    "nativescript-socketio": "^3.3.1",
    "nativescript-toasty": "^3.0.0-alpha.2",
    "nativescript-vue": "~2.6.4",
    "nativescript-vue-devtools": "^1.4.0",
    "tns-core-modules": "~6.5.18",
    "vuex": "^3.5.1",
    "vue-class-component": "^7.2.5",
    "vue-property-decorator": "^8.4.2"
  },
  "devDependencies": {
    "@babel/core": "~7.11.6",
    "@babel/preset-env": "~7.11.5",
    "@types/node": "^14.6.4",
    "babel-loader": "~8.1.0",
    "nativescript-dev-webpack": "^1.5.1",
    "nativescript-vue-template-compiler": "~2.8.1",
    "node-sass": "^4.14.1",
    "tns-platform-declarations": "^6.5.15",
    "ts-loader": "^8.0.3",
    "typescript": "^3.9.7",
    "vue": "^2.6.12",
    "vue-loader": "~15.9.3"
  }
}
